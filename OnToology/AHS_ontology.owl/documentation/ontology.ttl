@prefix : <http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#> .

<http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story> rdf:type owl:Ontology ;
                                                                            rdfs:comment "American Horror Story ontology" ,
                                                                                         "Ontology created by Ester Fernández Padrós" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasAppearanceIn
:hasAppearanceIn rdf:type owl:ObjectProperty ;
                 owl:inverseOf :hasCharacter ;
                 rdfs:domain :Character ;
                 rdfs:range :SeriesElement ;
                 rdfs:comment "Indicates what chapters or seasons the character appears." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasCharacter
:hasCharacter rdf:type owl:ObjectProperty ;
              rdfs:domain :SeriesElement ;
              rdfs:range :Character ;
              rdfs:comment "Indicates what characters has a chapter or a season." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasCharacterRole
:hasCharacterRole rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isPlayedBy ;
                  rdfs:domain :Actor ;
                  rdfs:range :Character ;
                  rdfs:comment "Indicates what character roles has an actor" .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasEpisode
:hasEpisode rdf:type owl:ObjectProperty ;
            owl:inverseOf :isFromSeason ;
            rdfs:domain :Season ;
            rdfs:range :Episode ;
            rdfs:comment "Indicates a season episode." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasKilled
:hasKilled rdf:type owl:ObjectProperty ;
           owl:inverseOf :isKilledBy ;
           rdfs:domain :Character ;
           rdfs:range :Character ;
           rdfs:comment "Indicates what characters has been killed by the character." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasMember
:hasMember rdf:type owl:ObjectProperty ;
           owl:inverseOf :isPartOf ;
           rdfs:domain :CharacterGroup ;
           rdfs:range :Character ;
           rdfs:comment "Indicates what characters are members of the group." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasPlace
:hasPlace rdf:type owl:ObjectProperty ;
          owl:inverseOf :isLocatedIn ;
          rdf:type owl:TransitiveProperty ;
          rdfs:domain :Place ;
          rdfs:range :Place ;
          rdfs:comment "Indicates a place that is located in the current place." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasSettingPlace
:hasSettingPlace rdf:type owl:ObjectProperty ;
                 owl:inverseOf :isSettingPlaceOf ;
                 rdfs:domain :Season ;
                 rdfs:range :Place ;
                 rdfs:comment "Indicates the place where is set the the season story." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isAfter
:isAfter rdf:type owl:ObjectProperty ;
         owl:inverseOf :isBefore ;
         rdf:type owl:IrreflexiveProperty ;
         rdfs:domain :SeriesElement ;
         rdfs:range :SeriesElement ;
         rdfs:comment "Indicates the element series that is after the current element." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isAfterEpisode
:isAfterEpisode rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isAfter ;
                owl:inverseOf :isBeforeEpisode ;
                rdf:type owl:IrreflexiveProperty ;
                rdfs:domain :Episode ;
                rdfs:range :Episode ;
                rdfs:comment "Indicates the episode that is after the current episode." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isAfterSeason
:isAfterSeason rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :isAfter ;
               owl:inverseOf :isBeforeSeason ;
               rdf:type owl:IrreflexiveProperty ;
               rdfs:domain :Season ;
               rdfs:range :Season ;
               rdfs:comment "Indicates the season that is after the current season." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isBefore
:isBefore rdf:type owl:ObjectProperty ,
                   owl:IrreflexiveProperty ;
          rdfs:domain :SeriesElement ;
          rdfs:range :SeriesElement ;
          rdfs:comment "Indicates the element series that is before the current element." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isBeforeEpisode
:isBeforeEpisode rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :isBefore ;
                 rdf:type owl:IrreflexiveProperty ;
                 rdfs:domain :Episode ;
                 rdfs:range :Episode ;
                 rdfs:comment "Indicates the episode that is before the current episode." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isBeforeSeason
:isBeforeSeason rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isBefore ;
                rdf:type owl:IrreflexiveProperty ;
                rdfs:domain :Season ;
                rdfs:range :Season ;
                rdfs:comment "Indicates the season that is before the current season." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isChildOf
:isChildOf rdf:type owl:ObjectProperty ;
           owl:inverseOf :isParentOf ;
           rdf:type owl:IrreflexiveProperty ;
           rdfs:domain :Character ;
           rdfs:range :Character ;
           rdfs:comment "Indicates who is the parent of a character." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isDirectedBy
:isDirectedBy rdf:type owl:ObjectProperty ;
              owl:inverseOf :isDirectorOf ;
              rdfs:domain :Episode ;
              rdfs:range :Director ;
              rdfs:comment "Indicates the episode director." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isDirectorOf
:isDirectorOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Director ;
              rdfs:range :Episode ;
              rdfs:comment "Indicates an episode directed by the director." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isFromSeason
:isFromSeason rdf:type owl:ObjectProperty ;
              rdfs:domain :Episode ;
              rdfs:range :Season ;
              rdfs:comment "Indicates the episode season." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isKilledBy
:isKilledBy rdf:type owl:ObjectProperty ;
            rdfs:domain :Character ;
            rdfs:range :Character ;
            rdfs:comment "Indicates who has killed the character." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isLocatedIn
:isLocatedIn rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             rdfs:domain :Place ;
             rdfs:range :Place ;
             rdfs:comment "Indicates where a place is located." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isParentOf
:isParentOf rdf:type owl:ObjectProperty ,
                     owl:IrreflexiveProperty ;
            rdfs:domain :Character ;
            rdfs:range :Character ;
            rdfs:comment "Indicates who is the child of a character." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isPartOf
:isPartOf rdf:type owl:ObjectProperty ;
          rdfs:domain :Character ;
          rdfs:range :CharacterGroup ;
          rdfs:comment "Indicates the group of a character." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isPlayedBy
:isPlayedBy rdf:type owl:ObjectProperty ;
            rdfs:domain :Character ;
            rdfs:range :Actor ;
            rdfs:comment "Indicates the actor who plays the role of the current character." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#isSettingPlaceOf
:isSettingPlaceOf rdf:type owl:ObjectProperty ;
                  rdfs:domain :Place ;
                  rdfs:range :Season ;
                  rdfs:comment "Indicates the season that has this place as setting place." .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasEpisodeNumber
:hasEpisodeNumber rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :hasNumber ;
                  rdfs:domain :Episode ;
                  rdfs:range xsd:unsignedInt .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasNumber
:hasNumber rdf:type owl:DatatypeProperty ;
           rdfs:domain :SeriesElement ;
           rdfs:range xsd:unsignedInt .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasProfession
:hasProfession rdf:type owl:DatatypeProperty ;
               rdfs:domain :Character ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasSeasonNumber
:hasSeasonNumber rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :hasNumber ;
                 rdfs:domain :Season ;
                 rdfs:range xsd:unsignedInt ;
                 rdfs:comment "Indicates the season number." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#hasSettingYear
:hasSettingYear rdf:type owl:DatatypeProperty ;
                rdfs:domain :Season ;
                rdfs:range xsd:unsignedInt ;
                rdfs:comment "Indicates the year that a season story is set." .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Actor
:Actor rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :hasCharacterRole ;
                             owl:someValuesFrom :Character
                           ] ;
       rdfs:subClassOf :RealPerson ;
       rdfs:comment "A person whose job is to play a character." ;
       rdfs:sameAs "https://dbpedia.org/ontology/Actor" .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Building
:Building rdf:type owl:Class ;
          rdfs:subClassOf :Place ;
          rdfs:comment "A place that is a building." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Character
:Character rdf:type owl:Class ;
           rdfs:subClassOf :Person ;
           owl:disjointWith :RealPerson ;
           rdfs:comment "A person that only exists in a fictional story." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#CharacterGroup
:CharacterGroup rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :hasMember ;
                                      owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                      owl:onClass :Character
                                    ] ;
                rdfs:comment "Group of fictional people according to their relationship." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#CharacterTrait
:CharacterTrait rdf:type owl:Class ;
                rdfs:comment "Qualities that characters can have." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Place ;
      rdfs:comment "A place that is a city." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Deceased
:Deceased rdf:type owl:Class ;
          rdfs:subClassOf :CharacterTrait ;
          rdfs:comment "The character that has this quality is dead." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Director
:Director rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :isDirectorOf ;
                                owl:someValuesFrom :Episode
                              ] ;
          rdfs:subClassOf :RealPerson ;
          rdfs:comment "A person whose job is to direct an audiovisual piece." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Episode
:Episode rdf:type owl:Class ;
         rdfs:subClassOf :SeriesElement ;
         owl:disjointWith :Season ;
         rdfs:comment "A fragment of a season." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Family
:Family rdf:type owl:Class ;
        rdfs:subClassOf :CharacterGroup ;
        owl:disjointWith :Organization ;
        rdfs:comment "Group of characters who belong to the same family" .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Ghost
:Ghost rdf:type owl:Class ;
       rdfs:subClassOf :Character ;
       rdfs:comment "A person who is a ghost." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Human
:Human rdf:type owl:Class ;
       rdfs:subClassOf :Character ;
       rdfs:comment "A person who is a human." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Murderer
:Murderer rdf:type owl:Class ;
          rdfs:subClassOf :CharacterTrait ;
          rdfs:comment "The character that has this quality is a murderer." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf :CharacterGroup ;
              rdfs:comment "Group of characters who belong to the same company or have formed some kind of alliance or band." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Person
:Person rdf:type owl:Class ;
        rdfs:comment "An intelligent individual." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Place
:Place rdf:type owl:Class ;
       rdfs:comment "Physical space where the series story can happen." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#RealPerson
:RealPerson rdf:type owl:Class ;
            rdfs:subClassOf :Person ;
            rdfs:comment "A person that exists." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Season
:Season rdf:type owl:Class ;
        rdfs:subClassOf :SeriesElement ;
        rdfs:comment "A season is a set of episodes." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#SeriesElement
:SeriesElement rdf:type owl:Class ;
               rdfs:comment "An element that constitutes a series." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#State
:State rdf:type owl:Class ;
       rdfs:subClassOf :Place ;
       rdfs:comment "A place that is a state from United States." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#SupernaturalPowersPossessor
:SupernaturalPowersPossessor rdf:type owl:Class ;
                             rdfs:subClassOf :CharacterTrait ;
                             rdfs:comment "The character that has this quality has supernatural powers." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Vampire
:Vampire rdf:type owl:Class ;
         rdfs:subClassOf :Character ;
         rdfs:comment "A person who is a vampire." .


###  http://www.semanticweb.org/e9_st/ontologies/2022/2/american_horror_story#Witch
:Witch rdf:type owl:Class ;
       rdfs:subClassOf :Character ;
       rdfs:comment "A person who is a witch." .


